<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets">
    <Import Project="Core Base.props" />
  </ImportGroup>
  <PropertyGroup Label="UserMacros">
    <PlatformDepsDir>$(WorldEditRoot)w32deps\</PlatformDepsDir>
    <LibSuffix>-d</LibSuffix>
    <WxWidgetsLibDir>vc14x_dll</WxWidgetsLibDir>
  </PropertyGroup>
  <PropertyGroup>
    <LinkIncremental>true</LinkIncremental>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <Link />
    <Link />
    <ResourceCompile>
      <PreprocessorDefinitions>_WIN32;%(PreprocessorDefinitions)</PreprocessorDefinitions>
    </ResourceCompile>
    <ClCompile>
      <RuntimeLibrary>MultiThreadedDebugDLL</RuntimeLibrary>
      <Optimization>Disabled</Optimization>
      <DebugInformationFormat>EditAndContinue</DebugInformationFormat>
    </ClCompile>
  </ItemDefinitionGroup>
  <ItemGroup>
    <BuildMacro Include="PlatformDepsDir">
      <Value>$(PlatformDepsDir)</Value>
    </BuildMacro>
    <BuildMacro Include="LibSuffix">
      <Value>$(LibSuffix)</Value>
    </BuildMacro>
    <BuildMacro Include="WxWidgetsLibDir">
      <Value>$(WxWidgetsLibDir)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>